@model IEnumerable<dynamic>

<h2>Anlysis Result</h2>

<table class="table table-hover table-bordered">
    <thead>
    <tr>
        <th>Date</th>
        <th>Content</th>
        <th>Mood</th>
        <th>Sentiment</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var entry in Model)
    {
        <tr>
            <td>@entry.Date.ToShortDateString()</td>
            <td>@entry.Content</td>
            <td>@entry.Mood</td>
            <td>@entry.Sentiment</td>
        </tr>
    }
    </tbody>
</table>